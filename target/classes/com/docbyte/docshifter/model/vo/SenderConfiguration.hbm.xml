<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>
    <class name="com.docbyte.docshifter.model.vo.SenderConfiguration" table="SENDERCONFIGURATION">
        <id name="id" column="ID">
            <generator class="native"/>
        </id>
        <property name="name"/>
        <property name="description"/>
        
        <many-to-one name="inputConfiguration" column="INPUTMODULECONFIGURATIONID" foreign-key="INPUTMODULECONFIGURATIONID" lazy="false"/>
    </class>
</hibernate-mapping>