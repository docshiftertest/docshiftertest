<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.docbyte.docshifter.model.vo.ChainConfiguration"
		table="CHAINCONFIGURATION">
		<id name="id" column="ID">
			<generator class="native" />
		</id>

		<property name="name" />
		<property name="description" />
		<property name="enabled" />
		<property name="printerName" />
		<property name="queueName" />

		<many-to-one name="rootNode" column="ROOTNODE"
			class="com.docbyte.docshifter.model.vo.Node" cascade="all" lazy="false" />
        
	</class>
</hibernate-mapping>