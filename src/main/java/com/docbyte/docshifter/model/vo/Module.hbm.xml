<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.docbyte.docshifter.model.vo.Module" table="MODULE">
		<id name="id" column="ID" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="name" />
		<property name="classname" />
		<property name="description" />
		<property name="type" />
		<property name="condition" />

		<set name="parameters" table="ModuleParams"
			lazy="false" cascade="all">
			<key column="MODULEID" />
			<many-to-many column="PARAMID"
				class="com.docbyte.docshifter.model.vo.Parameter" />
		</set>
	</class>
</hibernate-mapping>