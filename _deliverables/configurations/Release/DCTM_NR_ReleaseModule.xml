   <type>Release</type>
   <classname>com.docbyte.docshifter.receiver.operations.dctm.SaveOperation</classname>
   <parameters>
	<parameter>
	    <name>dctm_repository</name>
	    <description>Repository (should be the same as the input repository)</description>
	    <type>STRING</type>
	</parameter>
	<parameter>
	    <name>dctm_user</name>
	    <description>Repository User</description>
	    <type>STRING</type>
	</parameter>
	<parameter>
	    <name>dctm_pass</name>
	    <description>Repository Password</description>
	    <type>STRING</type>
	</parameter>
   </parameters>
</module>