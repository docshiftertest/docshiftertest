<?xml version="1.0" encoding="UTF-8"?>
<module>
   <name>File system input module</name>
   <description>Polls a file location and processes new files</description>
   <type>Input</type>
   <classname>com.docbyte.docshifter.sender.fs.scheduler.FSScheduler</classname>

   <parameters>
	<parameter>
	    <name>start_date</name>
	    <description>Start date (format: 01/01/2000 00:00)</description>
	    <type>TIME</type>
	</parameter>
	<parameter>
	    <name>end_date</name>
	    <description>End date (format: 01/01/2020 00:00)</description>
	    <type>TIME</type>
	</parameter>
	<parameter>
	    <name>frequency</name>
	    <description>Frequency (every x seconds)</description>
	    <type>INTEGER</type>
	</parameter>
	<parameter>
	    <name>location_path</name>
	    <description>Location path</description>
	    <type>STRING</type>
	</parameter>
	<parameter>
	    <name>afterImportAction</name>
	    <description>What to do after import? Possible values are: delete/nothing/move</description>
	    <type>STRING</type>
	</parameter>
	<parameter>
	    <name>moveDestination_path</name>
	    <description>Will move the imported file to this location if afterImportAction is set to move</description>
	    <type>STRING</type>
	</parameter>
   </parameters>
</module>