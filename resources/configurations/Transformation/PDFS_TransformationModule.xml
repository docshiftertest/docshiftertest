<?xml version="1.0" encoding="UTF-8"?>
<module>
 <name>PDFS Transformation</name>
   <description>PDF with Security Transformation</description>
   <type>Transformation</type>
   <classname>com.docbyte.docshifter.receiver.operations.pdf.PdfSTransformation</classname>
   <parameters>
	<parameter>
	    <name>RestrictPermissions</name>
	    <description>RestrictPermissions (true/false)</description>
	    <type>BOOLEAN</type>
	</parameter>
	<parameter>
	    <name>PermissionPassword</name>
	    <description>PermissionPassword</description>
	    <type>STRING</type>
	</parameter>
	<parameter>
	    <name>Printing</name>
	    <description>Restrict printing: 0: Not permitted / 1: Only Low resolution (150 DPI) / 2: Allow High resolution</description>
	    <type>INTEGER</type>
	</parameter>
	<parameter>
	    <name>Changes</name>
	    <description>Restrict changes: 0: Not permitted / 1: Allow Inserting, deleting and rotating pages / 2: Allow Filling in form fields / 3: Allow Commenting, filling in form fields / 4: Any except extracting pages</description>
	    <type>INTEGER</type>
	</parameter>
	<parameter>
	    <name>EnableCopyingOfContent</name>
	    <description>Enable Copying Of Content (true/false)</description>
	    <type>BOOLEAN</type>
	</parameter>
		<parameter>
	    <name>EnableTextAccessForAccessibilityTools</name>
	    <description>Enable Text Access (true/false)</description>
	    <type>BOOLEAN</type>
	</parameter>
   </parameters>
</module>